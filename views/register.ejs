<body>
    <div class="page-wrapper bg-gra-01 p-t-180 p-b-100 font-poppins">
        <div class="wrapper wrapper--w780">
            <div class="card card-3">
                <div class="card-heading"></div>
                <div class="card-body">
                    <h2 class="title">Registration Info</h2>
                    <form action="speaker/add" method="POST">
                        <div class="input-group">
                            <input class="input--style-3" type="number" placeholder="first name" name="_id" >
                        </div>
                        <div class="input-group">
                            <input class="input--style-3" type="text" placeholder="first name" name="fullName.fName">
                        </div>
                        <div class="input-group">
                            <input class="input--style-3" type="text" placeholder="last name" name="fullName.lName">
                        </div>
                        <div class="input-group">
                            <input class="input--style-3" type="text" placeholder="user name" name="username">
                        </div>
                        <div class="input-group">
                            <input class="input--style-3" type="age" placeholder="age" name="age">
                        </div>
                        <div class="input-group">
                            <input class="input--style-3 " type="password" placeholder="password" name="password">
                        </div> 
                      
                        <div class="input-group">
                            <input class="input--style-3" type="text" placeholder="street" name="address.street">
                        </div>
                        <div class="input-group">
                            <input class="input--style-3" type="text" placeholder="city" name="address.city">
                        </div>
                        <div class="input-group">
                            <input class="input--style-3" type="text" placeholder="country" name="address.country">
                        </div>
                       
                        <div class="p-t-10">
                            <input type="submit" value="sumbit" class="btn btn--pill btn--green">
                        </div>
                    </form>
                    <a href="/login"><span>already have an account ? log in </span></a>
                </div>
            </div>
        </div>
    </div>

    <!-- Vendor JS-->
    <script src="vendor/vendor1/select2/select2.min.js"></script>
